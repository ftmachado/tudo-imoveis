<form class="form-horizontal form-padding" method="post">

<fieldset>
    <legend>Informações Gerais</legend>
    <div class="form-group form-message-light">
        <label class="col-md-3 control-label" for="{{ form.fkImobiliariaId.vars['id'] }}">Imobiliária</label>
        <div class="col-md-4">
            {{ form_widget(form.fkImobiliariaId) }}
        </div>
    </div>

    <div class="form-group form-message-light">
        <label class="col-md-3 control-label" for="{{ form.fkEstadoId.vars['id'] }}">Estado</label>
        <div class="col-md-4">
            {{ form_widget(form.fkEstadoId) }}
        </div>
    </div>

    <div class="form-group form-message-light">
        <label class="col-md-3 control-label" for="{{ form.fkCidadeId.vars['id'] }}">Cidade</label>
        <div class="col-md-4">
            {{ form_widget(form.fkCidadeId) }}
        </div>
    </div>
</fieldset>

    <button class="btn btn-primary btn-rounded pull-right" type="submit">{{ button_label|default('Salvar') }}</button>

</form>